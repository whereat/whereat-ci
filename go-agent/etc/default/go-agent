# /etc/default/go-agent                                                                                

export PATH=/var/go/bin:$PATH
export GO_SERVER=go.whereat.io
export GO_SERVER_PORT=8153
export AGENT_WORK_DIR=/var/lib/${SERVICE_NAME:-go-agent}
export JAVA_HOME="/usr/lib/jvm/java-7-openjdk-amd64/jre" # SET_BY_GO_INSTALLER__DONT_REMOVE              
DAEMON=Y
VNC=N
